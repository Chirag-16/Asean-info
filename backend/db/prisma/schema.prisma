generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Resource {
  id                String @id @default(uuid())
  carrier           String @unique
  active            Int
  lastRun           String
  success           String
  rnf               String
  fail              String
  crawlFrequny      String
  durationTolaunch  String
  startTime         DateTime
  endTime           DateTime

}
